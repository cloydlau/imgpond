{
  "name": "imgpond",
  "version": "0.1.24",
  "author": "cloydlau",
  "description": "An image uploader based on filepond, cropping supported.",
  "keywords": [
    "img",
    "image",
    "upload",
    "uploader",
    "vue",
    "filepond",
    "crop",
    "cropper",
    "cropperjs"
  ],
  "private": false,
  "license": "MIT",
  "main": "dist/imgpond.umd.min.js",
  "scripts": {
