    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "element-ui": "^2.13.2",
    "pic-viewer": "^0.2.5",
    "plain-kit": "^0.2.3",
    "sass": "^1.26.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
