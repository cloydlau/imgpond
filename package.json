{
  "name": "imgpond",
  "version": "0.3.2",
  "author": "cloydlau",
  "description": "An editable and sortable image uploader powered by filepond & element",
  "keywords": [
    "img",
    "image",
    "upload",
    "uploader",
    "vue",
    "filepond",
    "crop",
    "cropper",
    "cropperjs",
    "sortable",
    "element"
  ],
  "private": false,
  "license": "MIT",
  "main": "dist/imgpond.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name imgpond src/main.js"
  },
  "dependencies": {
    "filepond": "4",
    "filepond-plugin-file-validate-type": "1",
    "filepond-plugin-image-preview": "4",
    "plain-kit": "0",
    "sortablejs": "1",
    "vue-cropperjs": "4",
    "vue-filepond": "6"
  },
  "peerDependencies": {
    "element-ui": "2",
    "pic-viewer": "0",
    "vue": "2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4",
    "@vue/cli-service": "4",
    "axios": "0",
    "babel-plugin-import": "1",
    "element-ui": "2",
    "json-editor-vue": "0",
    "pic-viewer": "0",
    "sass": "1",
    "sass-loader": "10",
    "vue": "2",
    "vue-template-compiler": "2"
  }
}
