{
  "name": "imgpond",
  "version": "0.3.2",
  "author": "cloydlau",
  "description": "An editable and sortable image uploader powered by filepond & element",
  "keywords": [
    "img",
    "image",
    "upload",
    "uploader",
    "vue",
    "filepond",
    "crop",
    "cropper",
    "cropperjs",
    "sortable",
    "element"
  ],
  "private": false,
  "license": "MIT",
  "main": "dist/imgpond.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name imgpond src/main.js"
  },
  "dependencies": {
    "filepond": "*",
    "filepond-plugin-file-validate-type": "*",
    "filepond-plugin-image-preview": "*",
    "plain-kit": "*",
    "sortablejs": "*",
    "vue-cropperjs": "*",
    "vue-filepond": "*"
  },
  "peerDependencies": {
    "element-ui": "*",
    "pic-viewer": "*",
    "vue": "*"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "*",
    "@vue/cli-service": "*",
    "axios": "*",
    "babel-plugin-import": "*",
    "element-ui": "*",
    "json-editor-vue": "*",
    "pic-viewer": "*",
    "sass": "*",
    "sass-loader": "*",
    "vue": "*",
    "vue-template-compiler": "*"
  }
